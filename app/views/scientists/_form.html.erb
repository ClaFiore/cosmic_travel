<%=render partial: "error_form", locals: {object: @scientist}%>

<%= form_for @scientist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :field_of_study %>
    <%= f.text_field :field_of_study%>
    <%= f.label :planet %>
    <%= f.collection_select :planet_ids, Planet.all, :id, :name%>
    <%= f.submit %>
    <% end %>